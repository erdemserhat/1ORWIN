<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape android:shape="rectangle">
            <solid android:color="@color/white"></solid>
            <corners android:radius="8dp"></corners>
            <size android:height="80dp" android:width="80dp"></size>

        </shape>
    </item>

    <item android:width="15dp" android:height="15dp" android:gravity="center" >
        <shape android:shape="oval">
            <size android:width="150dp" android:height="15dp"></size>
            <solid android:color="@color/black"></solid>

        </shape>

    </item>


    <item android:width="15dp" android:height="15dp" android:gravity="bottom|right" android:bottom="10dp" android:right="10dp">
        <shape android:shape="oval">
            <size android:width="150dp" android:height="15dp"></size>
            <solid android:color="@color/black"></solid>

        </shape>

    </item>

    <item android:width="15dp" android:height="15dp" android:gravity="bottom|left" android:bottom="10dp" android:left="10dp">
        <shape android:shape="oval">
            <size android:width="150dp" android:height="15dp"></size>
            <solid android:color="@color/black"></solid>

        </shape>

    </item>


    <item android:width="15dp" android:height="15dp" android:gravity="top|left" android:top="10dp" android:left="10dp">
        <shape android:shape="oval">
            <size android:width="150dp" android:height="15dp"></size>
            <solid android:color="@color/black"></solid>

        </shape>

    </item>

    <item android:width="15dp" android:height="15dp" android:gravity="top|right" android:top="10dp" android:right="10dp">
        <shape android:shape="oval">
            <size android:width="150dp" android:height="15dp"></size>
            <solid android:color="@color/black"></solid>

        </shape>

    </item>


</layer-list>